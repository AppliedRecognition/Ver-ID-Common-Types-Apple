{
    "$id": "http://appliedrecognition.com/schemas/FaceRecognitionTemplate",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "version": {
            "type": "integer"
        },
        "data": {
            "type": "string",
            "pattern": "^[A-Za-z0-9+/]+[=]{0,2}$"
        }
    },
    "required": [
        "version",
        "data"
    ]
}